{
  "name": "ts-app-template",
  "version": "0.1.0",
  "description": "A starter template for a typescript project",
  "scripts": {
    "start": "webpack serve",
    "build": "NODE_ENV=production webpack",
    "format": "prettier src/**/*.ts --write",
    "lint": "eslint src/**/*.ts",
    "test": "jasmine-ts --config=jasmine.json",
    "coverage": "nyc -r text -e .ts -x \"tests/*.test.ts\" npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/istherepie/ts-app-template.git"
  },
  "keywords": [
    "typescript",
		"frontend",
		"webpack",
		"jasmine",
		"test",
		"lint",
		"format",
		"minimal",
    "template"
  ],
  "author": "Steffen Park <hello@istherepie.com> (https://www.istherepie.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/istherepie/ts-app-template/issues"
  },
  "homepage": "https://github.com/istherepie/ts-app-template#readme",
  "devDependencies": {
    "@types/jasmine": "^3.9.0",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "css-loader": "^6.2.0",
    "eslint": "^7.32.0",
    "html-webpack-plugin": "^5.3.2",
    "jasmine": "^3.9.0",
    "jasmine-ts": "^0.4.0",
    "mini-css-extract-plugin": "^2.2.2",
    "nyc": "^15.1.0",
    "prettier": "2.3.2",
    "sass": "^1.39.0",
    "sass-loader": "^12.1.0",
    "ts-loader": "^9.2.5",
    "typescript": "^4.4.2",
    "webpack": "^5.52.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.1.0"
  }
}
