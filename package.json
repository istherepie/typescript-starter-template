{
  "name": "typescript-starter-template",
  "version": "0.1.0",
  "description": "A starter template for a typescript project",
  "scripts": {
    "start": "webpack serve",
    "build": "NODE_ENV=production webpack",
    "format": "prettier src/**/*.ts --write",
    "lint": "eslint src/**/*.ts",
    "test": "jasmine-ts src/**/*.spec.ts",
    "coverage": "nyc -r text -e .ts -x \"tests/*.test.ts\" npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/istherepie/typescript-starter-template.git"
  },
  "keywords": [
    "typescript",
    "frontend",
    "webpack",
    "jasmine",
    "test",
    "lint",
    "format",
    "minimal",
    "template"
  ],
  "author": "Steffen Park <dev@istherepie.com> (https://www.istherepie.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/istherepie/typescript-starter-template/issues"
  },
  "homepage": "https://github.com/istherepie/typescript-starter-template#readme",
  "devDependencies": {
    "@types/jasmine": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "css-loader": "^6.7.1",
    "eslint": "^8.11.0",
    "eslint-plugin-prettier": "^4.0.0",
    "html-webpack-plugin": "^5.5.0",
    "jasmine": "^4.0.2",
    "jasmine-ts": "^0.4.0",
    "mini-css-extract-plugin": "^2.6.0",
    "nyc": "^15.1.0",
    "prettier": "2.6.0",
    "sass": "^1.49.9",
    "sass-loader": "^12.6.0",
    "ts-loader": "^9.2.8",
    "typescript": "^4.6.2",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  }
}
