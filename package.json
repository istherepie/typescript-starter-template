{
  "name": "ts-app-template",
  "version": "1.0.0",
  "description": "A starter template for a typescript project",
  "scripts": {
    "start": "webpack serve",
    "build": "NODE_ENV=production webpack",
    "format": "prettier src/**/*.ts --write",
    "lint": "eslint src/**/*.ts",
    "test": "ts-node node_modules/jasmine/bin/jasmine --config=jasmine.json",
    "coverage": "nyc -r text -e .ts -x \"tests/*.test.ts\" npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/istherepie/ts-app-template.git"
  },
  "keywords": [
    "typescript",
    "template"
  ],
  "author": "Steffen Park <hello@istherepie.com> (https://www.istherepie.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/istherepie/ts-app-template/issues"
  },
  "homepage": "https://github.com/istherepie/ts-app-template#readme",
  "devDependencies": {
    "@types/jasmine": "^3.9.0",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "eslint": "^7.32.0",
    "html-webpack-plugin": "^5.3.2",
    "jasmine": "^3.9.0",
    "nyc": "^15.1.0",
    "prettier": "2.3.2",
    "ts-loader": "^9.2.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.2",
    "webpack": "^5.52.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.1.0"
  }
}
